{
  "name": "botyrest",
  "version": "0.0.3",
  "description": "RestFul API Framework",
  "main": "dist/index",
  "types": "dist/types/index",
  "scripts": {
    "prepare": "npm run build:prod",
    "build:dev": "rm -rf ./dist && mkdir ./dist && cp -R ./src/public/ ./dist && npx tsc --watch",
    "build:prod": "rm -rf ./dist && mkdir ./dist && cp -R ./src/public/ ./dist && npx tsc",
    "start:prod": "node ./dist/app.js",
    "start:dev": "node --watch ./dist/app.js"
  },
  "keywords": [
    "restful",
    "rest",
    "api",
    "framework",
    "typescript",
    "node"
  ],
  "author": "Pxndxs",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.16.14",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@fastify/middie": "^8.3.0",
    "@fastify/static": "^6.10.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.17.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0"
  }
}
